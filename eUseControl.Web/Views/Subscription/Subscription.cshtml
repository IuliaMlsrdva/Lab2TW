@{
    ViewBag.Title = "Subscription";
}




<body>
    <section class="services">
        <h2>Add films </h2>
        <div class="container-fluid">
            <div class="row">
               <table>
                  
                @using (Html.BeginForm("Product", "Subscription", FormMethod.Post, new { autocomplete  = "off" }))
                {
                    foreach(var prod in Model.Products)
                    {
                        <tr>
                            <td>@prod</td>
                            <td>
                                <button style="float:right" class=" btn btn-lg btn-primary" name=" btn"
                                        type="button" value="@prod">ADD
                                </button>
                            </td>
                        </tr>
                    }

                }
                </table>
            </div>
        </div>
    </section>
</body>

